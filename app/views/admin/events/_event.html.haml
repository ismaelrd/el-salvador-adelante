%tr
  %td
    = event.start_at.try(:strftime, '%d/%m/%Y')
  %td
    = event.name
  %td
    = link_to edit_admin_event_path(event), class: 'btn btn-primary btn-xs' do
      %i.glyphicon.glyphicon-edit
      = t('labels.edit')
  %td
    = link_to admin_event_path(event), class: 'btn btn-danger btn-xs', method: :delete, data: { confirm: t('labels.confirm_delete') } do
      %i.glyphicon.glyphicon-trash
      = t('labels.delete')
